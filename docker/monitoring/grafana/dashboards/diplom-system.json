{
  "dashboard": {
    "id": null,
    "title": "Diplom Distributed System Load Testing Dashboard",
    "tags": ["diplom", "load-testing", "distributed-system"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate (RPS)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(ingest_requests_total[1m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "P95 Latency",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(ingest_request_duration_seconds_bucket[1m])) by (le))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(ingest_requests_total{status=\"success\"}[1m])) / sum(rate(ingest_requests_total[1m])) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Active Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "processor_worker_pool_size",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Request Rate Over Time",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(ingest_requests_total[1m]))",
            "refId": "A",
            "legendFormat": "Total RPS"
          },
          {
            "expr": "sum(rate(ingest_requests_total{status=\"success\"}[1m]))",
            "refId": "B",
            "legendFormat": "Success RPS"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Latency Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(ingest_request_duration_seconds_bucket[1m])) by (le))",
            "refId": "A",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(ingest_request_duration_seconds_bucket[1m])) by (le))",
            "refId": "B",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(ingest_request_duration_seconds_bucket[1m])) by (le))",
            "refId": "C",
            "legendFormat": "P99"
          }
        ],
        "yAxes": [
          {
            "label": "Latency (seconds)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Queue Size",
        "type": "graph",
        "targets": [
          {
            "expr": "processor_queue_size",
            "refId": "A",
            "legendFormat": "Queue Size"
          }
        ],
        "yAxes": [
          {
            "label": "Messages",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Messages Processed",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(processor_messages_total[1m]))",
            "refId": "A",
            "legendFormat": "Processing Rate"
          },
          {
            "expr": "sum(rate(ingest_messages_processed_total[1m]))",
            "refId": "B",
            "legendFormat": "Ingest Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Messages/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-5m",
      "to": "now"
    },
    "refresh": "5s"
  }
} 